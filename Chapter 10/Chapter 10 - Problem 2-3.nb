 CellLabel->
  "Out[426]=",ExpressionUUID->"1b422963-0d8c-4d01-8dd8-f2e7bd663dcd"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"thirdpart", " ", "=", " ", 
  RowBox[{
   FractionBox[
    SuperscriptBox["Pi", "2"], "6"], "*", 
   SuperscriptBox[
    RowBox[{"(", "kbT", ")"}], "2"], "*", 
   RowBox[{"hprime", "[", "Ef", "]"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"again", ",", " ", 
     RowBox[{"this", " ", "is", " ", "0"}], ",", " ", 
     RowBox[{
      RowBox[{"I", "'"}], "m", " ", "just", " ", "putting", " ", "here", " ", 
      "so", " ", "we", " ", "can", " ", "be", " ", "methodical"}]}], " ", 
    "*)"}]}]}]], "Input",
 CellChangeTimes->{{3.785097301997678*^9, 3.785097335957827*^9}},
 CellLabel->
  "In[427]:=",ExpressionUUID->"277b7376-3acb-4c89-9948-e78b79c58b8a"],

Cell[BoxData["0"], "Output",
 CellChangeTimes->{3.785097336693858*^9, 3.785098420416336*^9},
 CellLabel->
  "Out[427]=",ExpressionUUID->"a34768ad-b817-49af-9f09-1b862029bf8d"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"sommerfield", " ", "=", " ", 
  RowBox[{
  "firstpart", " ", "+", " ", "secondpart", " ", "+", " ", 
   "thirdpart"}]}]], "Input",
 CellChangeTimes->{{3.785097339465478*^9, 3.7850973450834208`*^9}},
 CellLabel->
  "In[428]:=",ExpressionUUID->"78cf9a2e-9899-4a0b-82e8-30867a2166dd"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   SqrtBox["Ef"], " ", "n"}], "+", 
  FractionBox[
   RowBox[{"n", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "Ef"}], "+", "\[Mu]"}], ")"}]}], 
   SqrtBox["Ef"]]}]], "Output",
 CellChangeTimes->{3.7850973461336594`*^9, 3.785098422719146*^9},
 CellLabel->
  "Out[428]=",ExpressionUUID->"96ddbc09-30de-4902-a652-c7e530479d01"]
}, Open  ]],


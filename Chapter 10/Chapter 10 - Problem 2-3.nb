Cell["\<\
At T\[Rule]0, the Fermi-Dirac distribution is an inverse step function where \
the step occurs at Ef\
\>", "Text",
 CellChangeTimes->{{3.7850970560315266`*^9, 
  3.785097086527966*^9}},ExpressionUUID->"ce7688d3-52fd-4461-8811-\
300059a3c47c"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"firstpart", "=", " ", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"h", "[", "En", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"En", ",", "0", ",", "Ef"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.7850971427745323`*^9, 3.7850971599156895`*^9}},
 CellLabel->
  "In[421]:=",ExpressionUUID->"265d9136-f309-4ae5-85cf-db69157b1ec4"],

Cell[BoxData[
 RowBox[{
  SqrtBox["Ef"], " ", "n"}]], "Output",
 CellChangeTimes->{3.785097160212891*^9, 3.7850980991158223`*^9},
 CellLabel->
  "Out[421]=",ExpressionUUID->"a434917e-9d90-4883-940a-d6c1fc2835f3"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"secondpart", " ", "=", " ", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"\[Mu]", "-", "Ef"}], ")"}], "*", 
   RowBox[{"h", "[", "Ef", "]"}]}]}]], "Input",
 CellChangeTimes->{{3.785097181201749*^9, 3.7850971962225513`*^9}},
 CellLabel->
  "In[424]:=",ExpressionUUID->"6bfc34a3-7117-462e-b668-bed31eaf22ca"],

Cell[BoxData[
 FractionBox[
  RowBox[{"n", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "Ef"}], "+", "\[Mu]"}], ")"}]}], 
  SqrtBox["Ef"]]], "Output",
 CellChangeTimes->{3.7850971974582744`*^9, 3.785098101686911*^9, 
  3.785098412415738*^9},
 CellLabel->
  "Out[424]=",ExpressionUUID->"aa08a942-8280-4c12-8e2e-4e5ff6faa9b6"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"hprime", "[", "En_", "]"}], " ", "=", " ", 
  RowBox[{"D", "[", 
   RowBox[{
    RowBox[{"h", "[", "En", "]"}], ",", "En"}], "]"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"should", " ", "be", " ", "0"}], " ", "*)"}]}]], "Input",
 CellChangeTimes->{{3.7850972046530385`*^9, 3.7850972637439957`*^9}},
 CellLabel->
  "In[425]:=",ExpressionUUID->"da849291-debf-4012-9f7a-af8f966b38c9"],

Cell[BoxData["0"], "Output",
 CellChangeTimes->{3.785097261943781*^9, 3.785098109506996*^9, 
  3.785098415855534*^9},
 CellLabel->
  "Out[425]=",ExpressionUUID->"4888671d-8375-4e28-abdb-eea2baaa1dea"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"hprime", "[", "Ef", "]"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"should", " ", "be", " ", "0"}], "..."}], ".", "of"}], " ", 
    "course"}], "*)"}]}]], "Input",
 CellChangeTimes->{{3.7850972665185432`*^9, 3.785097277624873*^9}},

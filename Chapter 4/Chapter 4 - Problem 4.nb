  3.7836907625560865`*^9, 3.783690763778298*^9}},
 CellLabel->
  "In[250]:=",ExpressionUUID->"942f864e-7d34-4b06-90d3-ef9213453be3"],

Cell[BoxData[
 RowBox[{"3", "+", 
  FractionBox[
   RowBox[{"18", " ", 
    SqrtBox["2"], " ", 
    SuperscriptBox["\[Alpha]", 
     RowBox[{"3", "/", "2"}]]}], 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{"2", " ", "\[Alpha]"}]}], ")"}], 
    RowBox[{"5", "/", "2"}]]], "-", 
  FractionBox[
   RowBox[{"9", " ", 
    SqrtBox["2"], " ", 
    SqrtBox["\[Alpha]"]}], 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{"2", " ", "\[Alpha]"}]}], ")"}], 
    RowBox[{"3", "/", "2"}]]]}]], "Output",
 CellChangeTimes->{
  3.783690727936689*^9, 3.783690764121063*^9, 3.7836909002271276`*^9, {
   3.7836909523806*^9, 3.7836909923749914`*^9}, 3.7836910956779184`*^9, 
   3.7836912827618885`*^9},
 CellLabel->
  "Out[250]=",ExpressionUUID->"b8cf131f-4925-4e76-a25a-4baa0d30b518"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"N", "[", 
  RowBox[{"Solve", "[", 

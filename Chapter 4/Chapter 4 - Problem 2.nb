  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Phi]", "[", "x_", "]"}], "=", " ", 
   RowBox[{"x", "*", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "\[Alpha]"}], "*", 
      SuperscriptBox["x", "2"]}], "]"}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.7835475737578473`*^9, 3.7835476084115996`*^9}, 
   3.7835649943054266`*^9},
 CellLabel->"In[23]:=",ExpressionUUID->"0c0f73b8-cd87-4133-b24f-0407e623a5b4"]
